{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "title": "Genius",
            "salary": 70000.0,
            "equity": 0.5,
            "company_handle": "apple"
        }
    ],
    "required": [
        "title",
        "salary",
        "equity",
        "company_handle"
    ],
    "properties": {
        "title": {
            "$id": "#/properties/title",
            "type": "string",
            "title": "The title schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Genius"
            ]
        },
        "salary": {
            "$id": "#/properties/salary",
            "type": "number",
            "title": "The salary schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0.0,
            "examples": [
                70000.0
            ]
        },
        "equity": {
            "$id": "#/properties/equity",
            "type": "number",
            "maximum": 1,
            "title": "The equity schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0.0,
            "examples": [
                0.5
            ]
        },
        "company_handle": {
            "$id": "#/properties/company_handle",
            "type": "string",
            "title": "The company_handle schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "apple"
            ]
        },
        "requirements": {
            "$id": "/properties/requirements",
            "type": "array",
            "description": "The requirements schema",
            "default": [],
            "examples": [
                ["p", "q", "string"]
            ]
        },
        "token": {
            "$id": "/properties/token",
            "type": "string",
            "title": "The Token Schema ",
            "default": "",
            "examples": [
                "foo123"
            ]
        }
    },
    "additionalProperties": false
}