{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "Test",
            "pwd": "abc123",
            "first_name": "Test",
            "last_name": "Testy",
            "email": "test@test.com",
            "photo_url": "www.photo.com",
            "is_admin": true
        }
    ],
    "required": [
        "username",
        "pwd",
        "first_name",
        "last_name",
        "email"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Test"
            ]
        },
        "pwd": {
            "$id": "#/properties/pwd",
            "type": "string",
            "title": "The pwd schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "abc123"
            ]
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Test"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Testy"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "format": "email",
            "default": "",
            "examples": [
                "test@test.com"
            ]
        },
        "photo_url": {
            "$id": "#/properties/photo_url",
            "type": "string",
            "title": "The photo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "format": "uri",
            "default": "",
            "examples": [
                "www.photo.com"
            ]
        },
        "is_admin": {
            "$id": "#/properties/is_admin",
            "type": "boolean",
            "title": "the is_admin schema",
            "description": "Whether or not a user is an admin",
            "default": false,
            "examples": [
                false
            ]
        },
        "token": {
            "$id": "/properties/token",
            "type": "string",
            "title": "The Token Schema ",
            "default": "",
            "examples": [
                "foo123"
            ]
        }
    },
    "additionalProperties": false
}